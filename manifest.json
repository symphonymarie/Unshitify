{
  "manifest_version": 2,
  "name": "Unshitify-Searches",
  "version": "1.0",
  "description": "Show only search results prior to the Great AI Enshitiffication (Pre-2021).",
"permissions": [
    "tabs",
    "activeTab"
  ],
  "homepage_url": "https://github.com/symphonymarie/Unshitify",
  "icons": {
    "48": "icons/unshit-48.png"
  },
  "browser_specific_settings": {
